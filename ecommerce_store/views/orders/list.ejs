<h1>My Orders</h1>

<% if (!orders.length) { %>
  <p>No orders yet</p>
  <a href="/products">Shop now</a>
<% } else { %>
  <ul>
    <% orders.forEach(order => { %>
      <li>
        <a href="/orders/<%= order.id %>">
          Order #<%= order.id %>
        </a>
        - â‚¹<%= order.total_amount %>
        - <%= order.status %>
      </li>
    <% }) %>
  </ul>
<% } %>
